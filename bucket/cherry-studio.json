{
    "version": "1.6.2",
    "description": "A desktop client that supports for multiple LLM providers.",
    "homepage": "https://cherry-ai.com",
    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v1.6.2/Cherry-Studio-1.6.2-x64-setup.exe#/dl.7z",
            "hash": "999fbd32af59d2541a37abef3e1dd37dc59126a28e4921bec2dcb5ffbd1020f9",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\""
        },
        "arm64": {
            "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v1.6.2/Cherry-Studio-1.6.2-arm64-setup.exe#/dl.7z",
            "hash": "9d67f539e0e0ef2443414bf925842fa11801a4b30894e4ab3e6f852c6a76fbff",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-arm64.7z\" \"$dir\""
        }
    },
    "persist": [
        "data",
        "home"
    ],
    "post_install": [
        "$data_dir = Join-Path $env:USERPROFILE \".cherrystudio\"",
        "$persist_home_dir = Join-Path $persist_dir home",
        "& \"$bucketsdir\\$bucket\\scripts\\link.ps1\" $data_dir $persist_home_dir",
        "$appdata_dir = Join-Path $env:APPDATA \"CherryStudio\"",
        "$persist_data_dir = Join-Path $persist_dir data",
        "& \"$bucketsdir\\$bucket\\scripts\\link.ps1\" $appdata_dir $persist_data_dir"
    ],
    "shortcuts": [
        [
            "Cherry Studio.exe",
            "Cherry Studio"
        ]
    ],
    "checkver": {
        "github": "https://github.com/CherryHQ/cherry-studio"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v$version/Cherry-Studio-$version-x64-setup.exe#/dl.7z"
            },
            "arm64": {
                "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v$version/Cherry-Studio-$version-arm64-setup.exe#/dl.7z"
            }
        }
    }
}
