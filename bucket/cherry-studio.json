{
    "version": "1.6.6",
    "description": "A desktop client that supports for multiple LLM providers.",
    "homepage": "https://cherry-ai.com",
    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v1.6.6/Cherry-Studio-1.6.6-x64-setup.exe",
            "hash": "aba17ee7b91f1acf00457988d47048488af3f138580b56010f832311838a0aed",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\""
        },
        "arm64": {
            "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v1.6.6/Cherry-Studio-1.6.6-arm64-setup.exe",
            "hash": "2091399036dacca258540bec4eac5fc05201598e42328060c86f9abfdf9149b3",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-arm64.7z\" \"$dir\""
        }
    },
    "shortcuts": [
        [
            "Cherry Studio.exe",
            "Cherry Studio"
        ]
    ],
    "persist": [
        "data",
        "home"
    ],
    "post_install": [
        "$data_dir = Join-Path $env:USERPROFILE \".cherrystudio\"",
        "$persist_home_dir = Join-Path $persist_dir home",
        "& \"$bucketsdir\\$bucket\\scripts\\link.ps1\" $data_dir $persist_home_dir",
        "$appdata_dir = Join-Path $env:APPDATA \"CherryStudio\"",
        "$persist_data_dir = Join-Path $persist_dir data",
        "& \"$bucketsdir\\$bucket\\scripts\\link.ps1\" $appdata_dir $persist_data_dir"
    ],
    "checkver": {
        "github": "https://github.com/CherryHQ/cherry-studio"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v$version/Cherry-Studio-$version-x64-setup.exe"
            },
            "arm64": {
                "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v$version/Cherry-Studio-$version-arm64-setup.exe"
            }
        }
    }
}
